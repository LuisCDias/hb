
<div class="container-fluid dashboard">
      <div class="row-fluid">
        <%= render 'dashboards/menu' %>
        <div class="span10 ">
          <div class="dashboardheader">
            <h4 class="pull-left">SongVice</h4>

            <%= link_to "Request Advice", new_advice_request_path, class: "btn btn-large btn-info pull-right" %>
          </div>
          <div class="row-fluid dashboardmain">
           
           <table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Track URL</th>
      <th>Category</th>
      <th>Created</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @user_advice_requests.each do |advice_request| %>
      <tr>
        <td><%= advice_request.track_url %></td>
        <td><%= advice_request.category_id %></td>
        <td><%= time_ago_in_words(advice_request.created_at) %> ago </td>
        <td><%= link_to 'Show', advice_request %></td>
        <td><%= link_to 'Edit', edit_advice_request_path(advice_request) %></td>
        <td><%= link_to 'Destroy', advice_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
          </div><!--/row-->
        </div><!--/span-->
      </div><!--/row-->

      <hr>


    </div>

